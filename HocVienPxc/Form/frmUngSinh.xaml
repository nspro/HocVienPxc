                        </StackPanel>
                    </Canvas>
                    <StackPanel Height="25" Margin="10,0,7,2" VerticalAlignment="Bottom">
                        <Button x:Name="btn_SuaThongTinUngSinh" Content="Sửa" Width="50" VerticalAlignment="Bottom" HorizontalAlignment="Right" Margin="0" Height="25" Background="{DynamicResource {x:Static SystemColors.MenuHighlightBrushKey}}" BorderThickness="0.5">
                            <Button.BorderBrush>
                                <SolidColorBrush Color="#FF707070" Opacity="0.3"/>
                            </Button.BorderBrush>
                        </Button>
                    </StackPanel>
                </Grid>
            </TabItem>

            <!--Lịch sử ơn gọi-->
