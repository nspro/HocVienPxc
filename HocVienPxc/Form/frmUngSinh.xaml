                <Grid Background="{DynamicResource Color_Form_BackgroundF9F9F9F}">
                    <StackPanel Margin="10,10,10,30">
                        <DockPanel Height="431.667">
                            <ToolBar DockPanel.Dock="Top">
                                <ToggleButton Command="EditingCommands.ToggleBold" Name="btn_LichSuOnGoi_Bold">
                                    <Image Source="/HocVienPxc;component/Asset/Graphic/EditorIcon/img_editor_textBold.png" Width="16" Height="16" />
                                </ToggleButton>
                                <ToggleButton Command="EditingCommands.ToggleItalic" Name="btn_LichSuOnGoi_Italic">
                                    <Image Source="/HocVienPxc;component/Asset/Graphic/EditorIcon/img_editor_textItalic.png" Width="16" Height="16" />
                                </ToggleButton>
                                <ToggleButton Command="EditingCommands.ToggleUnderline" Name="btn_LichSuOnGoi_Undeline">
                                    <Image Source="/HocVienPxc;component/Asset/Graphic/EditorIcon/img_editor_textUnderline.png" Width="16" Height="16" />
                                </ToggleButton>
                                <Separator />
                                <ToggleButton Command="EditingCommands.AlignLeft" Name="btn_LichSuOnGoi_Left">
                                    <Image Source="/HocVienPxc;component/Asset/Graphic/EditorIcon/img_editor_text_align_left.png" Width="16" Height="16" />
                                </ToggleButton>
                                <ToggleButton Command="EditingCommands.AlignCenter" Name="btn_LichSuOnGoi_Center">
                                    <Image Source="/HocVienPxc;component/Asset/Graphic/EditorIcon/img_editor_text_align_center.png" Width="16" Height="16" />
                                </ToggleButton>
                                <ToggleButton Command="EditingCommands.AlignRight" Name="btn_LichSuOnGoi_Right">
